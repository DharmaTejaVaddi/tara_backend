[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/TaraFirst
ExecStart=/home/ubuntu/env/bin/gunicorn --timeout 600 --workers 3 --bind unix:/home/ubuntu/TaraFirst/Tara/Tara.sock Tara.wsgi:application


[Install]
WantedBy=multi-user.target